#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/xtt/Desktop/珈蓝电力/keylion/packages/keylion-cli/lib/node/node_modules:/Users/xtt/Desktop/珈蓝电力/keylion/packages/keylion-cli/lib/node_modules:/Users/xtt/Desktop/珈蓝电力/keylion/packages/keylion-cli/node_modules:/Users/xtt/Desktop/珈蓝电力/keylion/packages/node_modules:/Users/xtt/Desktop/珈蓝电力/keylion/node_modules:/Users/xtt/Desktop/珈蓝电力/node_modules:/Users/xtt/Desktop/node_modules:/Users/xtt/node_modules:/Users/node_modules:/node_modules:/Users/xtt/Desktop/珈蓝电力/keylion/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/xtt/Desktop/珈蓝电力/keylion/packages/keylion-cli/lib/node/node_modules:/Users/xtt/Desktop/珈蓝电力/keylion/packages/keylion-cli/lib/node_modules:/Users/xtt/Desktop/珈蓝电力/keylion/packages/keylion-cli/node_modules:/Users/xtt/Desktop/珈蓝电力/keylion/packages/node_modules:/Users/xtt/Desktop/珈蓝电力/keylion/node_modules:/Users/xtt/Desktop/珈蓝电力/node_modules:/Users/xtt/Desktop/node_modules:/Users/xtt/node_modules:/Users/node_modules:/node_modules:/Users/xtt/Desktop/珈蓝电力/keylion/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../lib/node/bin.js" "$@"
else
  exec node  "$basedir/../../lib/node/bin.js" "$@"
fi
