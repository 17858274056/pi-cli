#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/xtt/Desktop/珈蓝电力/keylion/node_modules/.pnpm/vite@4.0.4_@types+node@18.11.18_less@4.1.3_sass@1.58.2_terser@5.17.3/node_modules/vite/bin/node_modules:/Users/xtt/Desktop/珈蓝电力/keylion/node_modules/.pnpm/vite@4.0.4_@types+node@18.11.18_less@4.1.3_sass@1.58.2_terser@5.17.3/node_modules/vite/node_modules:/Users/xtt/Desktop/珈蓝电力/keylion/node_modules/.pnpm/vite@4.0.4_@types+node@18.11.18_less@4.1.3_sass@1.58.2_terser@5.17.3/node_modules:/Users/xtt/Desktop/珈蓝电力/keylion/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/xtt/Desktop/珈蓝电力/keylion/node_modules/.pnpm/vite@4.0.4_@types+node@18.11.18_less@4.1.3_sass@1.58.2_terser@5.17.3/node_modules/vite/bin/node_modules:/Users/xtt/Desktop/珈蓝电力/keylion/node_modules/.pnpm/vite@4.0.4_@types+node@18.11.18_less@4.1.3_sass@1.58.2_terser@5.17.3/node_modules/vite/node_modules:/Users/xtt/Desktop/珈蓝电力/keylion/node_modules/.pnpm/vite@4.0.4_@types+node@18.11.18_less@4.1.3_sass@1.58.2_terser@5.17.3/node_modules:/Users/xtt/Desktop/珈蓝电力/keylion/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../node_modules/.pnpm/vite@4.0.4_@types+node@18.11.18_less@4.1.3_sass@1.58.2_terser@5.17.3/node_modules/vite/bin/vite.js" "$@"
else
  exec node  "$basedir/../../../../node_modules/.pnpm/vite@4.0.4_@types+node@18.11.18_less@4.1.3_sass@1.58.2_terser@5.17.3/node_modules/vite/bin/vite.js" "$@"
fi
